{
  "name": "datav-vue",
  "version": "0.1.0",
  "description": "一个基于 Vue3.x 的数据可视化项目。",
  "scripts": {
    "cz": "npx git-cz",
    "gc": "bash ./scripts/gc.sh",
    "new": "plop --plopfile ./plop-templates/plopfile.js",
    "bootstrap": "yarn --frozen-lockfile",
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:js": "vue-cli-service lint",
    "lint:css": "stylelint 'src/**/*.(vue|css|less)' --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^5.0.2",
    "accounting": "^0.4.1",
    "axios": "^0.21.1",
    "core-js": "^3.13.1",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.5",
    "echarts": "^5.1.1",
    "element-plus": "^1.0.2-beta.46",
    "gsap": "^3.6.1",
    "html2canvas": "^1.0.0-rc.7",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "monaco-editor": "0.24.0",
    "particles.js": "^2.0.0",
    "shortid": "^2.2.16",
    "vue": "3.0.11",
    "vue-class-component": "^8.0.0-0",
    "vue-i18n": "^9.1.6",
    "vue-router": "4.0.8",
    "vuex": "4.0.1",
    "vuex-module-decorators": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/accounting": "^0.4.1",
    "@types/crypto-js": "^4.0.1",
    "@types/lodash": "^4.14.161",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/compiler-sfc": "3.0.11",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-vue": "^2.0.2",
    "chalk": "^4.1.1",
    "conventional-changelog-cli": "^2.1.1",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.10.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "monaco-editor-webpack-plugin": "3.1.0",
    "plop": "^2.7.4",
    "prettier": "2.3.1",
    "rimraf": "^3.0.2",
    "sass": "^1.34.1",
    "sass-loader": "^10.0.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.2.3",
    "webpack": "^4.44.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "vue-cli-service lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pengxiaotian/datav-vue.git"
  },
  "homepage": "http://pengxiaotian.github.io/datav",
  "author": "Allen",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
